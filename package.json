{
  "name": "terminal-commander",
  "version": "1.0.0",
  "description": "An interactive terminal command helper for programmers",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "terminal-commander": "./src/index.js",
    "tcmd": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node src/index.js",
    "lint": "eslint src/**/*.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "mkdir -p dist && cp -r src dist/ && cp package.json README.md LICENSE dist/"
  },
  "keywords": [
    "terminal",
    "cli",
    "command-line",
    "education",
    "learning",
    "gamification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.12",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
